<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.hkim00.moves">

    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE"/>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:name=".ParseApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.hkim00.moves.LocationActivity"></activity>
        <activity android:name="com.hkim00.moves.MoveDetailsActivity"></activity>
        <activity android:name="com.hkim00.moves.ProfileActivity" />
        <activity android:name="com.hkim00.moves.HomeActivity" />
        <activity android:name="com.hkim00.moves.FoodActivity" />
        <activity android:name="com.hkim00.moves.EventsActivity" />
        <activity android:name="com.hkim00.moves.SignUpActivity"></activity>
        <activity android:name="com.hkim00.moves.Intro1Activity"></activity>
        <activity android:name="com.hkim00.moves.Intro2Activity"></activity>
        <activity android:name="com.hkim00.moves.Intro3Activity"></activity>
        <activity android:name="com.hkim00.moves.LogInActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".QuestionnaireActivity"
            android:label="app_intro" />

        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />
        <meta-data
            android:name="com.google.android.maps.v2.API_KEY"
            android:value="@string/api_key" />
    </application>

</manifest>